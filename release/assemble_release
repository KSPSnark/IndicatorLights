echo Creating release file structure
mkdir -p GameData/IndicatorLights
cp ../files/ModuleManager.*.dll GameData
cp ../files/ModuleManagerLicense.md GameData
cp ../files/changelog.txt GameData/IndicatorLights
cp ../src/bin/Release/IndicatorLights.dll GameData/IndicatorLights
cp ../LICENSE GameData/IndicatorLights
cp ../README.md GameData/IndicatorLights
cp -R ../files/Agencies GameData/IndicatorLights
cp -R ../files/Config GameData/IndicatorLights
cp -R ../files/Flags GameData/IndicatorLights
cp -R ../files/Meshes GameData/IndicatorLights
cp -R ../files/Parts GameData/IndicatorLights
cp -r ../files/Patches GameData/IndicatorLights
echo Done.
